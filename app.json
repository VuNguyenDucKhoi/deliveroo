{
  "expo": {
    "name": "deliveroo",
    "slug": "deliveroo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00CCBB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00CCBB"
      },
      "package": "com.vunduckhoi.deliveroo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "703cd513-ada6-4c21-b1bf-cb0a0957180b"
      }
    },
    "owner": "vunduckhoi",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "eas": {
      "build": {
        "releaseChannel": "preview"
      }
    }
  }
}
